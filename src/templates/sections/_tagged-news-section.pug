.section.tagged-news-section
  .container
    .tagged-news-section__header
      h1.section__title.h2.section__title--limited 24 News Tagged to #Esports
    .three-column-grid
      each article in articles
        +article(article.name)
    .tagged-news-section__buttons
      button.button.button--action.button--md
        +icon('load-more')
        | Load more
      +return-button
