mixin social-links(modClass)
  .social-links(class= modClass ? `social-links--${modClass}`: '')
    p.social-links__title Follow us on
    .social-links__wrap
      a(href="/").social-links__link
        +icon('facebook-white', 'icon')
      a(href="/").social-links__link
        +icon('linkedin-white', 'icon')
