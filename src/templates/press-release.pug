---
title: enestech | IT-holding
lang: en
hasLink: false
isHeaderTransparent: true
---
extends layouts/_layout

block content
  main
    include sections/_press-release-hero-section

    hr.u-section-hr.u-my-48

    .press-release
      .section
        .container
          .section__header
            h2.h2.section__title.section__title--limited Accreditation
          .press-release__accreditations(class=`${accreditations.length < 2 ? 'press-release__accreditations--single':''}`)
            each accreditation in accreditations
              +accreditation(accreditation.name, accreditation.img, accreditation.date, accreditation.city)

      hr.u-section-hr.u-my-48

      .section
        .container
          .section__header
            h2.h2.section__title.section__title--limited Press releases
          .two-columns-layout
            .press-release__search
              include components/_search-menu
            .press-release__wrapper
              .press-release__articles
                each article in articles
                  +article(article.name, 'article--horizontal-md')
              .press-release__more
                button.button.button--action.button--md.press-release__button
                  +icon('load-more')
                  | Load more

      hr.u-section-hr.u-my-48.u-my-64-md

    include sections/_subscribe-press-room-section
