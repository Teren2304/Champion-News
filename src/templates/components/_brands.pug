.brands
  .brands__quote-list
    .js-slider-for
      each quote in barndsQuotes
        +quote(quote)

  .brands__logo-list
    .js-slider-nav
      div
        .brands__logo-wrap
          picture
            //source(srcset='img/brands/esi-logo.webp, img/brands/esi-logo@2.webp 2x' type="image/webp")
            source(srcset='https://business-portal-dev-bucket.s3.amazonaws.com/media/images/dotesports.original.png, https://business-portal-dev-bucket.s3.amazonaws.com/media/images/dotesports.original.png 2x' type="image/png")
            img.brands__logo(src='https://business-portal-dev-bucket.s3.amazonaws.com/media/images/dotesports.original.png' alt='esi-logo')
      div
        .brands__logo-wrap
          picture
            //source(srcset='img/brands/Reuters-Logo.webp, img/brands/Reuters-Logo@2.webp 2x' type="image/webp")
            source(srcset='img/brands/Reuters-Logo.png, img/brands/Reuters-Logo@2.png 2x' type="image/png")
            img.brands__logo(src='img/brands/Reuters-Logo.png' alt='Reuters-Logo')
      div
        .brands__logo-wrap
          picture
            //source(srcset='img/brands/forbes.webp, img/brands/forbes@2.webp 2x' type="image/webp")
            source(srcset='img/brands/forbes.png, img/brands/forbes@2.png 2x' type="image/png")
            img.brands__logo(src='img/brands/forbes.png' alt='forbes')
      div
        .brands__logo-wrap
          picture
            //source(srcset='img/brands/Mashable_Logo.webp, img/brands/Mashable_Logo@2.webp 2x' type="image/webp")
            source(srcset='img/brands/Mashable_Logo.png, img/brands/Mashable_Logo@2.png 2x' type="image/png")
            img.brands__logo(src='img/brands/Mashable_Logo.png' alt='Mashable_Logo')
      div
        .brands__logo-wrap
          picture
            //source(srcset='img/brands/Mashable_Logo.webp, img/brands/Mashable_Logo@2.webp 2x' type="image/webp")
            source(srcset='img/brands/Mashable_Logo.png, img/brands/Mashable_Logo@2.png 2x' type="image/png")
            img.brands__logo(src='img/brands/Mashable_Logo.png' alt='Mashable_Logo')
      div
        .brands__logo-wrap
          picture
            //source(srcset='img/brands/forbes.webp, img/brands/forbes@2.webp 2x' type="image/webp")
            source(srcset='img/brands/forbes.png, img/brands/forbes@2.png 2x' type="image/png")
            img.brands__logo(src='img/brands/forbes.png' alt='forbes')
